{
  "version": 2,
  "builds": [
    {
      "src": "solana-bridge.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mint-nft",
      "methods": ["POST"],
      "dest": "/solana-bridge.js"
    },
    {
      "src": "/blockhash",
      "methods": ["POST"],
      "dest": "/solana-bridge.js"
    },
    {
      "src": "/send-tx",
      "methods": ["POST"],
      "dest": "/solana-bridge.js"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "/solana-bridge.js"
    },
    {
      "src": "/.*",
      "dest": "/solana-bridge.js"
    }
  ]
}
