{
  "version": 2,
  "builds": [
    {
      "src": "api/solana-bridge.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mint-nft",
      "methods": ["POST"],
      "dest": "/api/solana-bridge.js"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "/api/solana-bridge.js"
    }
  ]
}
