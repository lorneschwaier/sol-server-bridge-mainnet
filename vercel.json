{
  "version": 2,
  "builds": [
    {
      "src": "api/*.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mint-nft",
      "methods": ["POST"],
      "dest": "/api/mint-nft.cjs"
    },
    {
      "src": "/api/health",
      "methods": ["GET"],
      "dest": "/api/health.cjs"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/health.cjs"
    }
  ]
}
