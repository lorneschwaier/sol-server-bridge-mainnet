{
  "version": 2,
  "builds": [
    {
      "src": "api/mint-nft.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mint-nft",
      "methods": ["POST"],
      "dest": "/api/mint-nft.cjs"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/api/mint-nft.cjs"
    }
  ]
}
